<h1><%= @category.title %></h1>
	<ul>
		<% @category.items.each do |item| %>
		    <%if item.inventory? %>
		  		<li><%= item.title %> | <%= number_to_currency(item.price.to_f/100) %> | 
			  	<%if current_user %>
			  		<%= button_to 'Add to Cart', line_items_path(item_id: item) %>
			  		</li>
				<% end %>
		   <% end %>
	   <% end %>
	</ul>